{
    "compilerOptions": {
        "jsx": "react",
        "module": "commonjs",
        "noImplicitAny": true,
        "outDir": "./dist/",
        "lib": ["es2020", "dom", "dom.iterable"],
        "esModuleInterop": true,
        "sourceMap": true,
        "strict": true,
        "target": "es5",
        "moduleResolution": "node",
        "baseUrl": "."
    },
    "include": ["src"],
    "exclude": ["node_modules"]
}
